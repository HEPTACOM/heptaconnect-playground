{
    "name": "heptacom/heptaconnect-playground-shopware-platform",
    "version": "0.0.1",
    "description": "Sandbox environment to demonstrate specific heptaconnect features in a shopware environment",
    "bin": [
        "bin/shopware"
    ],
    "config": {
        "platform": {
            "php": "7.4.3"
        }
    },
    "repositories": {
        "heptaconnect-demo": {
            "type": "path",
            "url": "../heptaconnect/**"
        },
        "heptaconnect-source-bridge-shopware-platform": {
            "type": "vcs",
            "url": "../sources/bridge-shopware-platform"
        },
        "heptaconnect-source-core": {
            "type": "vcs",
            "url": "../sources/core"
        },
        "heptaconnect-source-dataset-base": {
            "type": "vcs",
            "url": "../sources/dataset-base"
        },
        "heptaconnect-source-dataset-ecommerce": {
            "type": "vcs",
            "url": "../sources/dataset-ecommerce"
        },
        "heptaconnect-source-lib-sdk": {
            "type": "vcs",
            "url": "../sources/lib-sdk"
        },
        "heptaconnect-source-portal-base": {
            "type": "vcs",
            "url": "../sources/portal-base"
        },
        "heptaconnect-source-portal-local-shopware-platform": {
            "type": "vcs",
            "url": "../sources/portal-local-shopware-platform"
        },
        "heptaconnect-source-storage-base": {
            "type": "vcs",
            "url": "../sources/storage-base"
        },
        "heptaconnect-source-storage-native": {
            "type": "vcs",
            "url": "../sources/storage-native"
        },
        "heptaconnect-source-storage-shopware-dal": {
            "type": "vcs",
            "url": "../sources/storage-shopware-dal"
        }
    },
    "autoload": {
        "psr-4": {
            "Heptacom\\HeptaConnect\\Playground\\ShopwarePlatform\\": "src/"
        }
    },
    "require": {
        "php": ">=7.4",
        "composer/package-versions-deprecated": "1.8.0",
        "shopware/core": "^6.4",
        "shopware/storefront": "^6.4",
        "shopware/administration": "^6.4",
        "heptacom/heptaconnect-bridge-shopware-platform": "~0.8.0",
        "heptacom/heptaconnect-playground-dataset": "*",
        "heptacom/heptaconnect-playground-portal": "*",
        "heptacom/heptaconnect-playground-portal-extension": "*",
        "heptacom/heptaconnect-portal-local-shopware-platform": "~0.8.0",
        "heptacom/heptaconnect-lib-sdk": "~0.8.0"
    }
}
