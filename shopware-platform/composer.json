{
    "name": "heptacom/heptaconnect-playground-shopware-platform",
    "description": "Sandbox environment to demonstrate specific heptaconnect features in a shopware environment",
    "bin": [
        "bin/shopware"
    ],
    "repositories": [
        {
            "url": "../heptaconnect/**",
            "type": "path"
        }
    ],
    "autoload": {
        "psr-4": {
            "Heptacom\\HeptaConnect\\Playground\\ShopwarePlatform\\": "src/"
        }
    },
    "require": {
        "php": ">=7.4",
        "shopware/core": "6.2.3",
        "shopware/storefront": "6.2.3",
        "heptacom/heptaconnect-bridge-shopware-platform": "@dev",
        "heptacom/heptaconnect-playground-dataset": "@dev",
        "heptacom/heptaconnect-playground-portal": "@dev",
        "heptacom/heptaconnect-playground-portal-extension": "@dev"
    },
    "minimum-stability": "dev"
}
